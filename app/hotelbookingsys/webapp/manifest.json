{
  "_version": "1.65.0",
  "sap.app": {
    "id": "hotelbooking.hotelbookingsys",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.5",
      "toolsId": "c44e4f29-dfbe-4cd9-aaa8-cd1f8fa1cea5"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/hotelbookingsrv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "HotelBookingSys-display": {
          "semanticObject": "HotelBookingSys",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.127.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "hotelbooking.hotelbookingsys.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "hotelbooking.hotelbookingsys.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHotelBooking",
          "pattern": ":?query:",
          "target": [
            "TargetHotelBooking"
          ]
        },
        {
          "name": "RouteUserview",
          "pattern": "userview",
          "target": [
            "TargetRouteUserview"
          ]
        },
        {
          "name": "RouteAdminview",
          "pattern": "adminview",
          "target": [
            "TargetRouteAdminview"
          ]
        }
      ],
      "targets": {
        "TargetHotelBooking": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "HotelBooking",
          "viewName": "HotelBooking"
        },
        "TargetRouteUserview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "User",
          "viewName": "User"
        },
        "TargetRouteAdminview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Admin",
          "viewName": "Admin"
        }
      }
    },
    "rootView": {
      "viewName": "hotelbooking.hotelbookingsys.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
